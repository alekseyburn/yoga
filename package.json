{
  "name": "teamplate",
  "version": "16.0.0",
  "private": true,
  "description": "start teamplate",
  "devDependencies": {
    "@babel/preset-env": "^7.6.3",
    "@htmlacademy/editorconfig-cli": "1.x",
    "autoprefixer": "^9.6.4",
    "babel-preset-env": "^1.7.0",
    "browser-sync": "^2.26.7",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-babel": "^8.0.0",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-htmlmin": "^5.0.1",
    "gulp-iife": "^0.4.0",
    "gulp-imagemin": "^6.1.1",
    "gulp-plumber": "1.2.x",
    "gulp-postcss": "8.0.x",
    "gulp-rename": "^1.4.0",
    "gulp-sass": "4.0.x",
    "gulp-sourcemaps": "2.6.x",
    "gulp-svgstore": "^7.0.1",
    "gulp-uglyfly": "^1.4.2",
    "gulp-webp": "^4.0.1",
    "imagemin-mozjpeg": "^8.0.0",
    "stylelint": "11.0.x",
    "stylelint-config-htmlacademy": "^0.1.4"
  },
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint",
    "build": "gulp build",
    "start": "gulp start"
  },
  "browserslist": [
    "last 2 versions",
    "IE 11",
    "Firefox ESR"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "engines": {
    "node": "8.15"
  },
  "dependencies": {
    "eslint-config-airbnb-standard": "^3.0.1"
  }
}
